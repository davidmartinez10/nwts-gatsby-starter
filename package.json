{
  "name": "nwts-gatsby-starter",
  "displayName": "NW.ts Tools Gatsby Starter",
  "private": true,
  "version": "0.0.0",
  "description": "NW.js starter template with Typescript, Gatsby and NW.ts-Tools. It works with the VSCode debugger out-of-the-box.",
  "scripts": {
    "postinstall": "node node_modules/nwts-tools/patch-zx.js",
    "start": "npm run dev-hmr",
    "patch-codecs": "nwts-patch-codecs",
    "dev-hmr": "zx scripts/dev-hmr.mjs",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "build": "zx scripts/build.mjs",
    "compile-to-bin": "zx scripts/compile.mjs",
    "package": "zx scripts/package.mjs"
  },
  "license": "MIT",
  "dependencies": {
    "@types/chrome": "^0.0.197",
    "@types/node": "^17.0.45",
    "@types/nw.js": "^0.13.16",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "gatsby": "^4.24.4",
    "nw": "0.65.1-sdk",
    "nwts-tools": "^0.3.5",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "typescript": "^4.8.3",
    "wait-on": "^6.0.1",
    "zx": "^7.1.1"
  }
}
